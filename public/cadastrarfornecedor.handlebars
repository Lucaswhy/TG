    <link rel='stylesheet' type='text/css' media='screen' href='cadastrarfornecedor.css'>

	<h2>Cadastrar Fornecedor</h2>

	<form class="form-cadastro" method="POST" action="/cadastrarfornecedor">

    <input type="hidden" name="CPF" value="resultado">

        <div class="form-row">
            <div class="col-md-2 mb-3">
                <label for="validationDefault02">Código do Fornecedor</label>
                <input type="number" class="form-control" id="validationDefault02" placeholder="Cod. Fornecedor" required name="CodFornecedor">
            </div>
        </div>

        <div class="form-row">
            <div class="col-md-4 mb-3">
                <label for="validationDefault02">Razão Social</label>
                <input type="text" class="form-control" id="validationDefault02" placeholder="Razão Social" required name="RazaoSocial">
            </div>
            <div class="col-md-2 mb-3">
                <label for="validationDefault02">CNPJ</label>
                <input type="text" class="form-control cnpj" id="validationDefault02" placeholder="CNPJ" required name="CPFn">
            </div>
  		</div>
        
        <div class="form-row">
            <div class="col-md-5 mb-3">
                <label for="email">E-mail</label>
                <input type="email" class="form-control" id="email" placeholder="nome@exemplo.com" required name="Email">
            </div>
            <div class="col-md-3 mb-3">
                <label for="email">Telefone</label>
                <input type="text" class="form-control phone" id="email" placeholder="" required name="Telefone">
            </div>
        </div>

		<h3>Endereço</h3>

        <div class="form-row">
            <div class="col-md-4 mb-3">
                <label for="validationDefault01">Endereço</label>
                <input type="text" class="form-control" id="validationDefault01" placeholder="Endereço" name="Endereco">
            </div>
            <div class="col-md-2 mb-3">
                <label for="validationDefault02">CEP</label>
                <input type="text" class="form-control cep" id="validationDefault02" placeholder="CEP" name="CEP">
            </div>
        </div>

        <div class="form-row">
            <div class="col-md-2 mb-3">
                <label for="validationDefault01">Número</label>
                <input type="number" class="form-control" id="validationDefault01" placeholder="Número" name="Numero">
            </div>
            <div class="col-md-4 mb-3">
                <label for="validationDefault02">Bairro</label>
                <input type="text" class="form-control" id="validationDefault02" placeholder="Bairro" name="Bairro">
            </div>
        </div>

        <div class="form-row">
            <div class="col-md-4 mb-3">
                <label for="validationDefault03">Cidade</label>
                <input type="email" class="form-control" id="validationDefault03" placeholder="Cidade" name="Cidade">
            </div>
            <div class="form-group col-md-2.5">
                <label for="inputEstado">Estado</label>
                <select id="inputEstado" class="form-control" name="Estado">
                    <option selected>Selecione o estado...</option>
                    <option>AC</option>
                    <option>AL</option>
                    <option>AP</option>
                    <option>AM</option>
                    <option>BA</option>
                    <option>CE</option>
                    <option>DF</option>
                    <option>ES</option>
                    <option>GO</option>
                    <option>MA</option>
                    <option>MT</option>
                    <option>MS</option>
                    <option>MG</option>
                    <option>PA</option>
                    <option>PB</option>
                    <option>PR</option>
                    <option>PE</option>
                    <option>PI</option>
                    <option>RJ</option>
                    <option>RN</option>
                    <option>RS</option>
                    <option>RO</option>
                    <option>RR</option>
                    <option>SC</option>
                    <option>SP</option>
                    <option>SE</option>
                    <option>TO</option>
                </select>
            </div>
        </div>

        <button class="btn btn-primary" type="submit">Enviar</button>
    </form>